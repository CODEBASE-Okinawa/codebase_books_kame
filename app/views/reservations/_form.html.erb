<div class="row">
  <div class="col-md-6 col-md-offset-3">
  

    <%= form_with(model:[@lending_book,@reservation_book], local: true)  do |f| %>

      <%= f.date_select :reserved_start, {}, class: 'form-control' %>
      <label class="control-label" for="">から</label>

      <%= f.date_select :reserved_end, {}, class: 'form-control' %>
      <label class="control-label" for="">まで</label>

      <%= f.submit "本を借りる", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>